{
    "name": "vz-env-cli",
    "version": "1.0.4",
    "description": "CLI tool for managing and validating environment variables",
    "main": "dist/vz-env.js",
    "bin": {
        "vz-env": "dist/vz-env.js"
    },
    "scripts": {
        "start": "node dist/vz-env.js",
        "lint": "eslint .",
        "format": "prettier --write .",
        "build": "esbuild bin/vz-env.js --bundle --platform=node --outfile=dist/vz-env.js"
    },
    "keywords": [
        "env",
        "envs",
        "vars",
        "environment",
        "variables",
        "manager",
        "sync",
        "config"
    ],
    "files": [
        "dist/",
        "README.md",
        "LICENSE",
        "package.json"
    ],
    "author": "vzhV",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/vzhV/vz-env.git"
    },
    "homepage": "https://github.com/vzhV/vz-env#readme",
    "bugs": {
        "url": "https://github.com/vzhV/vz-env/issues"
    },
    "dependencies": {
        "chalk": "^5.3.0",
        "commander": "^12.1.0",
        "fs": "^0.0.1-security",
        "readline": "^1.3.0"
    },
    "devDependencies": {
        "esbuild": "^0.24.0",
        "eslint": "^9.17.0",
        "prettier": "^3.4.2"
    }
}
